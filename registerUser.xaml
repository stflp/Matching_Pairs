<Window x:Class="TEMA_1_PAIRS.registerUser"
        Closing="Window_Closing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TEMA_1_PAIRS"
        mc:Ignorable="d"
        Title="registerUser" Height="270" Width="500">
    <Grid>
        <Button x:Name="back" Content="Back" Height="30" Width="100" Margin="120, 20" HorizontalAlignment="Left" VerticalAlignment="Bottom" Click="back_Click"></Button>
        <Button x:Name="regUser" Content="Register User" Height="30" Width="100" Margin="120, 20" HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="regUser_Click"></Button>
        <TextBox Name="regUsrBox" Height="22" Width="150" Margin="100" HorizontalAlignment="Left" VerticalAlignment="Center" KeyDown="regUsrBox_KeyDown"></TextBox>
        <Image Name="myImage" Height="100" Width="100" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="50, 50"></Image>
            <Button Content=">" Height="20" FontWeight="10" VerticalAlignment="Center" HorizontalAlignment="Right" Click="nextImage"></Button>
    </Grid>
</Window>
